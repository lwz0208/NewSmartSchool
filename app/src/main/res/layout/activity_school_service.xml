<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F3F2"
    android:orientation="vertical"
    tools:context="com.wust.newsmartschool.ui.SchoolServiceActivity">

    <RelativeLayout
        android:id="@+id/title_workfrag_noticedetail"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_top_bar"
        android:background="@color/top_bar_normal_bg"
        android:gravity="center_vertical">

        <include
            android:id="@+id/back_workfrag_noticedetail"
            layout="@layout/ease_commom_back_btn" />

        <TextView
            style="@style/common_topbar_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="校园服务" />

    </RelativeLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:layout_below="@id/title_workfrag_noticedetail"
        android:background="@color/top_bottom_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <View
            android:layout_width="fill_parent"
            android:layout_height="2px"
            android:layout_marginTop="27dp"
            android:background="@color/dividercolor" />

        <LinearLayout
            android:id="@+id/ll_teacher_head_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/frammenu_pressornot_bg_selector"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginTop="7dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/school_dinner" />

                    <TextView
                        android:id="@+id/order_meal"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:enabled="true"
                        android:gravity="center"
                        android:text="校园订餐"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:layout_width="5px"
                android:layout_height="38dp"
                android:layout_gravity="center"
                android:src="@drawable/horizontal_sperater" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/frammenu_pressornot_bg_selector"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginTop="7dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/school_repair" />

                    <TextView
                        android:id="@+id/online_repair"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="在线报修"
                        android:textColor="#22201F"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2px"
            android:background="@color/dividercolor" />
        <!--
        <LinearLayout
            android:id="@+id/ll_teacher_head_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/frammenu_pressornot_bg_selector"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginTop="7dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/school_macket" />

                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="在线报修"
                        android:textColor="#22201F"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <ImageView
                    android:layout_width="5px"
                    android:layout_height="38dp"
                    android:layout_gravity="center"
                    android:src="@drawable/horizontal_sperater" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/frammenu_pressornot_bg_selector"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginBottom="7dp"
                        android:layout_marginTop="7dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/school_life" />

                    <TextView
                        android:id="@+id/emptyroom"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:clickable="true"
                        android:gravity="center"
                        android:text="校园生活"
                        android:textColor="#22201F"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="2px"
                android:background="@color/dividercolor" />
        </LinearLayout>

        <com.myView.CommonListView
            android:id="@+id/school_list1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13.5dp"
            android:background="@color/dividercolor"
            android:divider="@color/dividercolor"
            android:dividerHeight="2px"
            android:paddingBottom="2px"
            android:paddingTop="2px" >
        -->

        <com.wust.newsmartschool.views.CommonListView
            android:id="@+id/school_list2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13.5dp"
            android:background="@color/dividercolor"
            android:divider="@color/dividercolor"
            android:dividerHeight="2px"
            android:paddingBottom="2px"
            android:paddingTop="2px"></com.wust.newsmartschool.views.CommonListView>
    </LinearLayout>

</LinearLayout>